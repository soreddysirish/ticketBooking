 <div class="container">
	<div class="row">
		<div class="col-md-4  col-xs-12 col-md-push-9">
			<div class="panel panel-login" style="background-color:rgba(163, 165, 166, 0.28)">
				<div class="panel-heading" style="background-color:rgba(163, 165, 166, 0.28)">
					<div class="row">
						<div class="col-xs-6">
							<a href="#" class="active" id="login-form-link">Login</a>
						</div>
						<div class="col-xs-6">
							<a href="#" id="register-form-link">Register</a>
						</div>
					</div>
				</div>
				<div class="section-head">
				<h2 class ="textForLogin">A world of <span class="jenna-sue">Entertainment Awaits.</span></h2>
					
				</div>
				<div class="panel-body">
					<div class="row">
						<div class="col-lg-12 col-xs-12">
							<form id="login-form" action="#" method="post" role="form" style="display: block;">
								<div class="form-group">
									<input type="text" name="Your_name" id="" tabindex="1" class="form-control" placeholder="Email id" value="">
								</div>
								<div class="form-group">
									<input type="password" name="password" id="password" tabindex="2" class="form-control" placeholder="Password">
									<a href="#" tabindex="5" class="forgot-password" style="margin-top: 2%">Forgot Password?</a>
								</div>
								<div class="form-group">
									<div class="row">
										<div class="col-sm-6 col-md-12">
											<input type="submit" name="login-submit" id="login_submit" tabindex="4" class="form-control btn" value="Log In">
										</div>
									</div>
								</div>
							</form>
							<form id="register-form" action="#" method="post" role="form" style="display: none;">
								<div class="form-group">
									<input type="text" name="username" id="username" tabindex="1" class="form-control" placeholder="Your name" value="">
								</div>
								<div class="form-group">
									<input type="email" name="email" id="email" tabindex="1" class="form-control" placeholder="Email Address" value="">
								</div>
								<div class="form-group">
									<input type="password" name="password" id="password" tabindex="2" class="form-control" placeholder="Password">
								</div>
								<div class="form-group">
									<input type="password" name="confirm-password" id="confirm-password" tabindex="2" class="form-control" placeholder="Confirm Password">
								</div>
								<div class="form-group">
									<div class="row">
										<div class="col-sm-6 col-sm-offset-3">
											<input type="submit" name="register-submit" id="register-submit" tabindex="4" class="form-control btn btn-success" value="Register Now">
										</div>
									</div>
								</div>
							</form>
							<div class="row">
								<div class="col-md-8 col-md-push-2">
									<div class="divider">
										<span class="or">OR</span>
									</div>
									<div class="socialButtons dividerMarginEtop">
										<a class="btn btn-block btn-social  btn-twitter"><span class="fa fa-twitter"></span>Twitter</a>
									</div>
									<div class="socialButtons">
										<a class="btn btn-block btn-social  btn-google"><span class="fa fa-google"></span>Google+</a>
									</div>
									<div class="socialButtons">
										<a class="btn btn-block btn-social  btn-facebook"><span class="fa fa-facebook	"></span>Facebook</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>



